msgid "_File"
msgstr "_Fitxer"

#: ../main-window.cs:323
msgid "_Help"
msgstr "A_juda"

#: ../main-window.cs:329
msgid "_Open..."
msgstr "_Obri..."

#: ../main-window.cs:331
msgid "Load a log of system calls from a file"
msgstr "Carrega un registre de crides del sistema des d'un fitxer"

#: ../main-window.cs:335
msgid "Save _As..."
msgstr "_Anomena i alça..."

#: ../main-window.cs:337
msgid "Save the displayed log to a file"
msgstr "Alça el registre que es mostra en un fitxer"

